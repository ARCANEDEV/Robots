{
    "name": "arcanedev/robots",
    "description": "Robots generator",
    "keywords": ["robots", "laravel"],
    "license": "MIT",
    "authors": [
        {
            "name": "ARCANEDEV",
            "email": "arcanedev.maroc@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.4.0",
        "illuminate/support": "~4.2"
    },
    "require-dev": {
        "orchestra/testbench": "~2.2",
        "phpunit/phpcov": "~2.0",
        "mockery/mockery": "~0.9"
    },
    "autoload": {
        "psr-4": {
            "Arcanedev\\Robots\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Arcanedev\\Robots\\Tests\\": "tests/"
        }
    },
    "minimum-stability": "stable"
}
